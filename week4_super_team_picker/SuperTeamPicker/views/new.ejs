<%- include("./partials/header") %>

<form action="/cohorts/new" method = "POST">
    <br>
    <div>
        <label for="logo_url">Logo Url</label>
        <div class="input-group col-12 mb-4">
            <input type="text" name="logo_url" class="form-control" id="basic-url" aria-describedby="basic-addon3">
        </div>
    </div> 

    <div>
        <label for="team_name">Name</label>
        <div class="input-group col-12 mb-4">
            <input type="text" name="team_name" class="form-control" id="basic-url" aria-describedby="basic-addon3">
        </div>
    </div> 

    <div class="input-group">
        <label for="members_name">Members</label>
        <div class="input-group row-5 col-12 mb-4">
            <textarea class="form-control" name="members_name" aria-label="With textarea"></textarea>
        </div>   
    </div>
    
    <button type="submit" class="btn btn-outline-primary float-right mr-3">Submit</button>
    
</form>




  
